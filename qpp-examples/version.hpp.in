#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
//
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
//
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
//
// Version      AAAAABBBBBCCCCCDDDE
//
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
//
#define QPP_EXAMPLES_VERSION       $qpp_examples.version.project_number$ULL
#define QPP_EXAMPLES_VERSION_STR   "$qpp_examples.version.project$"
#define QPP_EXAMPLES_VERSION_ID    "$qpp_examples.version.project_id$"
#define QPP_EXAMPLES_VERSION_FULL  "$qpp_examples.version$"

#define QPP_EXAMPLES_VERSION_MAJOR $qpp_examples.version.major$
#define QPP_EXAMPLES_VERSION_MINOR $qpp_examples.version.minor$
#define QPP_EXAMPLES_VERSION_PATCH $qpp_examples.version.patch$

#define QPP_EXAMPLES_PRE_RELEASE   $qpp_examples.version.pre_release$

#define QPP_EXAMPLES_SNAPSHOT_SN   $qpp_examples.version.snapshot_sn$ULL
#define QPP_EXAMPLES_SNAPSHOT_ID   "$qpp_examples.version.snapshot_id$"
